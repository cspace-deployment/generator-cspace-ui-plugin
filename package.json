{
  "name": "generator-cspace-ui-plugin",
  "version": "0.0.20",
  "description": "Scaffold out a CollectionSpace UI plugin",
  "author": "Ray Lee <rhlee@berkeley.edu>",
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": ["yeoman-generator"],
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.22.0",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.5",
    "nsp": "^3.2.1",
    "prettier": "^1.12.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.7.0"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "gulp-rename": "^1.2.2",
    "lodash": "^4.17.10",
    "yeoman-generator": "^2.0.5",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "ray-lee/generator-cspace-ui-plugin",
  "license": "ECL-2.0"
}
